{
	"name":"Deepseek chat",
	"creator":"Mike Anderson",
	"description":"Example operation that chats with a DeepSeek endpoint via LangChainAdapter (OpenAI-compatible route)",
	"dateCreated":"2025-09-16T00:00:00Z",
	"dateModified": "2025-09-16T00:00:00Z",
	"keywords":["LLM", "model", "deepseek"],
	"operation": {
		"adapter": "langchain:openai",
		"featured":true,

		"input":{
			"type":"object",
			"properties":{
				"prompt":{
					"type":"string",
					"description":"The prompt for the model."
				},
				"model":{
					"type":"string",
					"description":"Optional model name (defaults to deepseek-chat)",
					"default":"deepseek-chat"
				},
				"systemPrompt":{
					"type":"string",
					"description":"System prompt to set the AI's behaviour and context"
				},
				"apiKey":{
					"type":"string",
					"secret":true,
					"description":"API key for authentication"
				},
				"url":{
					"type":"string",
					"description":"Base URL (defaults to https://api.deepseek.com/v1)",
					"default":"https://api.deepseek.com/v1"
				}
			}
		},
		"output":{
			"type":"object",
			"properties":{
				"response":{
					"type":"string",
					"description":"The LLM response"
				},
				"think":{
					"type":"string",
					"description":"Model thought process"
				}
			}
		}
	}
}

